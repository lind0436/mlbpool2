<div metal:use-macro="view.layout">
    <div metal:fill-slot="main_content">

        <div class="container">
            <div class="row upcoming">
                <div class="col-md-3"></div>
                <div class="col-md-6">


                    <h1>MLBPool2 Standings History</h1>

                    <h2>Season</h2>

                    <div class="seasons-played" tal:repeat="a seasons">
                        <ul>
                            <li tal:condition="a.season"><a href="/standings/season/${a.season}">${a.season}</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>