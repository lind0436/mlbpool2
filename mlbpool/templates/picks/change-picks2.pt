<div metal:use-macro="view.layout">
    <div metal:fill-slot="main_content">


        <div class="container">
            <div class="row upcoming">
                <div class="col-md-3"></div>
                <div class="col-md-6">


                    <h1>Change your picks for the ${season} season.</h1>

                   <h2>Hi, ${first_name}! </h2>

                    <div class="alert alert-warning" role="alert">If the season has not started yet, you can
                        change as many picks as you want (except the tiebreaker) without penalty after you press
                        submit below.  Visit the account page after you submit to view your picks or change your picks.
                        You can only change up to 14 of your picks at the All-Star Break.
                        These changes will only be worth half points and do qualify for double points if unique.</div>

                    <form class="form-horizontal" action="" method="POST">
                    <div class="form-group">
                         <table class="table table-striped">
                          <thead>
                            <tr>
                                <th>Pick Name</th>
                                <th>Your Pick</th>
                                <th>Unique?</th>
                                <th>Change Pick?</th>
                                <th>New Pick</th>
                            </tr>
                          </thead>

                          <tbody>
                           <span tal:repeat="p all_picks" tal:switch="p.pick_type">

                            <tr tal:case="1">

                                <td tal:condition="(p.rank==1)" tal:content="string:${p.league} ${p.division} Winner"></td>
                                <td tal:condition="(p.rank==2)" tal:content="string:${p.league} ${p.division} Second Place"></td>
                                <td tal:condition="(p.rank==5)" tal:content="string:${p.league} ${p.division} Last Place"></td>


                                <td tal:content="string:${p.name}"></td>
                                <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_al_east_winner_pick" class="form-control" id="change_al_east_winner_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 1) and (p.rank==1)">
                                    <select name="al_east_winner_pick" class="form-control" id="al_east_winner_pick">
                                    <option tal:repeat="a al_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 1) and (p.rank==2)">
                                    <select name="al_east_second_pick"
                                    class="form-control" id="al_east_second_pick">
                                    <option tal:repeat="a al_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 1) and (p.rank==5)">
                                    <select name="al_east_last_pick"
                                    class="form-control" id="al_east_last_pick">
                                    <option tal:repeat="a al_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 2) and (p.rank==1)">
                                    <select name="al_central_winner_pick" class="form-control" id="al_central_winner_pick">
                                    <option tal:repeat="a al_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 2) and (p.rank==2)">
                                    <select name="al_central_second_pick"
                                    class="form-control" id="al_central_second_pick">
                                    <option tal:repeat="a al_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 2) and (p.rank==5)">
                                    <select name="al_central_last_pick"
                                    class="form-control" id="al_central_last_pick">
                                    <option tal:repeat="a al_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 3) and (p.rank==1)">
                                    <select name="al_west_winner_pick" class="form-control" id="al_west_winner_pick">
                                    <option tal:repeat="a al_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 3) and (p.rank==2)">
                                    <select name="al_west_second_pick"
                                    class="form-control" id="al_west_second_pick">
                                    <option tal:repeat="a al_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==0) and (p.division_id == 3) and (p.rank==5)">
                                    <select name="al_west_last_pick"
                                    class="form-control" id="al_west_last_pick">
                                    <option tal:repeat="a al_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 1) and (p.rank==1)">
                                    <select name="nl_east_winner_pick" class="form-control" id="nl_east_winner_pick">
                                    <option tal:repeat="a nl_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 1) and (p.rank==2)">
                                    <select name="nl_east_second_pick"
                                    class="form-control" id="nl_east_second_pick">
                                    <option tal:repeat="a nl_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 1) and (p.rank==5)">
                                    <select name="nl_east_last_pick"
                                    class="form-control" id="nl_east_last_pick">
                                    <option tal:repeat="a nl_east" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 2) and (p.rank==1)">
                                    <select name="nl_central_winner_pick" class="form-control" id="nl_central_winner_pick">
                                    <option tal:repeat="a nl_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 2) and (p.rank==2)">
                                    <select name="nl_central_second_pick"
                                    class="form-control" id="nl_central_second_pick">
                                    <option tal:repeat="a nl_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 2) and (p.rank==5)">
                                    <select name="nl_central_last_pick"
                                    class="form-control" id="nl_central_last_pick">
                                    <option tal:repeat="a nl_central" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 3) and (p.rank==1)">
                                    <select name="nl_west_winner_pick" class="form-control" id="nl_west_winner_pick">
                                    <option tal:repeat="a nl_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 3) and (p.rank==2)">
                                    <select name="nl_west_second_pick"
                                    class="form-control" id="nl_west_second_pick">
                                    <option tal:repeat="a nl_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                                <td tal:condition="(p.league_id==1) and (p.division_id == 3) and (p.rank==5)">
                                    <select name="nl_west_last_pick"
                                    class="form-control" id="nl_west_last_pick">
                                    <option tal:repeat="a nl_west" value="${a.team_id}">${a.name}</option>
                                    </select><br></td>
                            </tr>

                            <tr tal:case="3">
                               <td tal:content="string:${p.league} Most wins"></td>
                                  <td tal:content="string:${p.name}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_wins_pick">
                                    <option tal:repeat="a al_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>

                                <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_wins_pick">
                                    <option tal:repeat="a nl_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>
                            </tr>

                           <tr tal:case="2">
                               <td tal:content="string:${p.league} Most losses"></td>
                                  <td tal:content="string:${p.name}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                               <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                               <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_losses_pick">
                                    <option tal:repeat="a al_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>

                               <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_losses_pick">
                                    <option tal:repeat="a nl_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="4">
                               <td tal:content="string:${p.league} Home Runs"></td>
                                  <td tal:content="string:${p.firstname} ${p.lastname}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_hr_pick">
                                    <option tal:repeat="a al_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_hr_pick">
                                    <option tal:repeat="a nl_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="5">
                               <td tal:content="string:${p.league} Batting Average"></td>
                                  <td tal:content="string:${p.firstname} ${p.lastname}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_ba_pick">
                                    <option tal:repeat="a al_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_ba_pick">
                                    <option tal:repeat="a nl_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="6">
                               <td tal:content="string:${p.league} RBIs"></td>
                                  <td tal:content="string:${p.firstname} ${p.lastname}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_rbi_pick">
                                    <option tal:repeat="a al_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_rbi_pick">
                                    <option tal:repeat="a nl_hitter_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="7">
                               <td tal:content="string:${p.league} Pitcher Wins"></td>
                                  <td tal:content="string:${p.firstname} ${p.lastname}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_p_wins_pick">
                                    <option tal:repeat="a al_pitcher_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_p_wins_pick">
                                    <option tal:repeat="a nl_pitcher_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="8">
                               <td tal:content="string:${p.league} ERA"></td>
                                  <td tal:content="string:${p.firstname} ${p.lastname}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_era_pick">
                                    <option tal:repeat="a al_pitcher_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_era_pick">
                                    <option tal:repeat="a nl_pitcher_list" value="${a.player_id}">${a.firstname}
                                        ${a.lastname} ${a.position} ${a.team_abbr}</option>
                                    </select><br> </td>
                            </tr>

                            <tr tal:case="9">
                               <td tal:content="string:${p.league} Wildcard"></td>
                                  <td tal:content="string:${p.name}"></td>
                                  <td tal:condition="(p.multiplier==1)" tal:content="string:No"></td>
                                  <td tal:condition="(p.multiplier==2)" tal:content="string:Yes"></td>
                                <td> <select name="change_pick" class="form-control" id="change_pick">
                                    <option tal:repeat="c change_pick" value="${c}">${c}</option></td>
                                <td tal:condition="(p.league_id == 0)"><select class="form-control"
                                                                                 name="al_wildcard1_pick">
                                    <option tal:repeat="a al_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>
                                  <td tal:condition="(p.league_id == 1)"><select class="form-control"
                                                                                 name="nl_wildcard1_pick">
                                    <option tal:repeat="a nl_wildcard_list" value="${a.team_id}">${a.name}</option>
                                    </select><br> </td>
                            </tr>

                           </span>
                          </tbody>
                         </table>

                        <h3>Change Your Picks</h3>

                        <button type="submit" class="btn-success">Submit Your Changes</button>
                    </div>
                    </form>
                </div>

            </div>

        </div>

    </div>

</div>
